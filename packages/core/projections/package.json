{
	"name": "@polaris.gl/projection",
	"version": "0.1.0-alpha.21",
	"keywords": [
		"unity3d-package"
	],
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"csharp": "csharp",
	"author": "西萌",
	"description": "",
	"lint-staged": {
		"src/**/*.js": [
			"prettier --write",
			"eslint --fix",
			"git add"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"type": "module",
	"license": "MIT",
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"package.json",
		"README.md",
		"dist/*",
		"csharp/*Projection.cs"
	],
	"scripts": {
		"build": "./scripts/build.sh",
		"rebuild": "npm run build",
		"dist": "npm run rebuild && webpack --config webpack.config.prod.js && npm run test",
		"start": "node --max-old-space-size=8192 server.js",
		"test": "jest",
		"autofix": "eslint --fix ./",
		"lint": "eslint ./",
		"v": "webpack -v"
	},
	"devDependencies": {
		"typescript": ">4.4.0"
	}
}
