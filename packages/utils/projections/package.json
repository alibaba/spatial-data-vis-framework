    },
    "optionalDependencies": {
        "babel-eslint": "^10.0.1",
        "compression": "^1.7.3",
        "eslint": "^5.15.1",
        "eslint-config-standard": "^12.0.0",
        "eslint-plugin-import": "^2.16.0",
